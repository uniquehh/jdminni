.goBack{
    text-align: center;
    line-height: 45px;
}
.search{
  border-bottom: 2rpx solid #E5E5E5;
  position: sticky;
  top: 0;
  background-color: white;
  z-index: 999;
}
.search image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 1.5vh;
}
.search-box{
  margin: 0 1.5vh;
  display: flex;
  align-items: center;
  padding: 1vh 0;
}
.search-box .btn{
  margin-left: 1vh;
  width: 6vh;
  text-align: center;
}
.search-box .inputbox{
  flex: 1 1 auto;
  display: flex;
  background-color: #F7F7F7;
  height: 5vh;
  padding: 0 2vh;
  border-radius: 6vh;
  align-items: center;
}
.inputbox text{
  height: 35px;
  line-height: 35px;
  padding-right: 5px;
  margin-right: 5px;
  border-right: 1px solid #b2d4f0;
}
.testmove{
    padding-bottom: 10px;
    white-space: nowrap;
    display: flex;
}
.test{
    position: relative;
    height: 60px;
    width:60px;
    margin:10px 0 10px 10px;
    flex-shrink: 0;
}
.navCont{
    position: absolute;
    top:0;
    background-color: rgba(0, 0, 0, 0.5);
    width:100%;
    height: 100%;
    justify-content: center;
    align-items: center;
    display: flex;
    color:white;
}
.img{width: 100%; height: 100%;}
.s_list{
    display: flex;
    padding:15px 15px 15px 30px;
    font-size: 13px;
    border-bottom:1px solid #eee;
}
.line{
    background-color: #ccc;
    width: 1px; 
    height: 15px;
    margin:2px 20px 0 20px;
}
.screen{
    display: flex;
}
.screen1{
    margin:0 60px;
}
.dropdown{
    margin-top:8px;
    margin-left:4px;
    height: 0;
    width: 0;
    border-left:4px solid transparent;
    border-right:4px solid transparent;
    border-top:4px solid red;
    border-bottom:4px solid transparent;
}
.dropdown2{
  float: right;
  margin-top:13px;
  margin-right: 10px;
  height: 0;
  width: 0;
  border-left:4px solid transparent;
  border-right:4px solid transparent;
  border-top:4px solid #CCCCCC;
  border-bottom:4px solid transparent;
}
.m_category{
    padding-left:10px;
    display: flex;
    height: 50px;
    align-items: center;
    overflow: visible;
}
.m_item{
    flex-shrink: 0;
    font-size: 11px;
    background-color: #F2F2F7;
    width: 80px;
    box-sizing: border-box;
    height: 30px;
    line-height: 30px;
    margin-right:10px;
    text-align: center;
    z-index: 4;
    position: relative;
    overflow: visible;
}
.m_item::after{
  content: "";
  display: block;
  clear: both;
}
.dropCont{
  position: fixed;
  z-index: 99;
  top: 250px;
  left:0rpx;
  width: 750rpx;
  background-color: rgba(0, 0, 0, 0.3);
  padding-bottom:calc(100vh - 250px);
}
.click{
    background-color: #F2F2F7;
   padding:3px 5px;
}
.click>view{
    font-size: 13px;
    background-color: #FBEEEC;
    text-align: center;
    padding:6px 0;
}
.click image{
    width: 40px;
    height: 20px;
    vertical-align: middle;
}
.list{
    padding:10px;
}
.layout{
    display: flex;
}
.laycont{
    border-bottom: 1px solid #ccc;
    padding-bottom:10px;
    padding-left:10px;
}
.layimg{
    width: 110px;
    height: 110px;
    flex-shrink: 0;
}
.title{
    font-size: 12px;
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.tag{
    background-color: #F2F2F7;
    font-size: 12px;
    color: #999;
    margin-right:5px;
    padding:0 4px;
}
.m_pri{
    margin-top:5px;
    color:#F3280D;
    font-size: 10px;
}
.price{
    font-size:16px;
}
.discount{
    border:1px solid #F3280D;
    border-radius: 2px;
    padding:0 2px;
    margin-left:5px;
}
.comment{
    font-size: 12px;
    color:#999;
}
.store{
    margin-top:5px;
}