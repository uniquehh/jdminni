.index{
  display: flex;
  flex-direction: column;
}
.container{
  display: flex;
  flex: 1 1 auto;
}
.acurr{
  background-color: white;
}
.aside{
  background-color: #F7F7F7;
  display: flex;
}
.aside scroll-view{
  height: 1006rpx;
}
.menu{
  padding: 20rpx 20rpx;
}
.main{
  flex: 1 1 auto;
}
.search{
  border-bottom: 2rpx solid #E5E5E5;
  position: sticky;
  top: 0;
  background-color: white;
  z-index: 999;
}
.search cover-image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}
.search-box{
  margin: 0 16rpx;
  display: flex;
  padding: 16rpx 0;
}
.search-box .btn{
  margin-left: 10rpx;
  height: 70rpx;
  line-height: 70rpx;
}
.search-box .inputbox{
  flex: 1 1 auto;
  display: flex;
  background-color: #F7F7F7;
  height: 70rpx;
  padding: 0 20rpx;
  border-radius: 40rpx;
  align-items: center;
}
.inputbox text{
  height: 70rpx;
  line-height: 70rpx;
  padding-right: 10rpx;
  margin-right: 10rpx;
}
.container .head{
  margin-bottom: 10rpx;
  padding: 0 20rpx;
}
.container .list{
  padding: 20rpx 40rpx;
}
.main .wrap{
  height: 954rpx;
}
.container .itembox{
  margin-bottom: 20rpx;
}
.container .itembox::after{
  content: "";
  display: block;
  clear: both;
}
.container .list .item{
  float: left;
  width: 33%;
  height: 200rpx;
  text-align: center;
  margin-bottom: 16rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;
}
.item image{
  width: 100%;
}