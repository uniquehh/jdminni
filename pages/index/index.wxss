.index{
  display: flex;
  flex-direction: column;
}
.container{
  display: flex;
  flex: 1 1 auto;
}
.acurr{
  background-color: white;
}
.aside{
  background-color: #F7F7F7;
  display: flex;
}
.aside scroll-view{
  height: 91.4vh;
}
.menu{
  padding: 20rpx 20rpx;
  max-width: 135rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.main{
  flex: 1 1 auto;
}
.main scroll-view{
  height: 91.4vh;
}
.search{
  border-bottom: 2rpx solid #E5E5E5;
  position: sticky;
  top: 0;
  background-color: white;
  z-index: 999;
}
.search image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 1.5vh;
}
.search-box{
  margin: 0 1.5vh;
  display: flex;
  align-items: center;
  padding: 1vh 0;
}
.search-box .btn{
  margin-left: 1vh;
  width: 6vh;
  text-align: center;
}
.search-box .inputbox{
  flex: 1 1 auto;
  display: flex;
  background-color: #F7F7F7;
  height: 5vh;
  padding: 0 2vh;
  border-radius: 6vh;
  align-items: center;
}
.container .head{
  margin-bottom: 10rpx;
  padding: 0 20rpx;
}
.container .list{
  padding: 20rpx 40rpx;
}
.container .itembox{
  margin-bottom: 20rpx;
}
.container .itembox::after{
  content: "";
  display: block;
  clear: both;
}
.container .list .item{
  float: left;
  width: 33%;
  height: 200rpx;
  text-align: center;
  margin-bottom: 16rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;
}
.item image{
  width: 100%;
}