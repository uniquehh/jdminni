/* pages/spcar/spcar.wxss */
.bordertop{
  border-top: 2rpx solid #F5F5F5;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 999;
}
.headnav{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 70rpx;
  width: 750rpx;
  border-bottom: 2rpx solid #F5F5F5;
}
.headnav .title{
  font-size: 36rpx;
}
.goback,.more{
  width: 40rpx;
  vertical-align: middle;
  padding: 0 20rpx;
}
.spcar{
  background-color: #F2F2F2;
  height: 100%;
  min-width: 375px;
  font-size: 32rpx;
}
.position{
  margin-bottom: 30rpx;
}
.posibox{
  background-color: white;
  height: 120rpx;
  width: 750rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom-left-radius: 40rpx;
  border-bottom-right-radius: 40rpx;
}
.posiboxtwo{
  justify-content: center;
}
.posibox .left{
  padding-left: 30rpx;
}
.posibox .right{
  padding-right: 15rpx;
}
.posibox image{
  margin-right: 20rpx;
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
}
.posibox .right>text{
  margin-right: 15rpx;
}
.spcar .head{
  background-color: white;
  padding: 20rpx 30rpx;
  margin-bottom: 40rpx;
}
.head .comnum{
  margin-right: 20rpx;
}
.nolgone{
  font-size: 28rpx;
}
.loginbtn{
  width: 70rpx;
  padding: 10rpx 20rpx;
  background-color: red;
  color: white;
  text-align: center;
  border-radius: 30rpx;
  margin-left: 20rpx;
  font-size: 28rpx;
}
.nolgtwo{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 400rpx;
}
.nolgtwo image{
  width: 150rpx;
  height: 150rpx;
  margin-bottom: 20rpx;
}
.list {
  margin-bottom: 40rpx;
}
.agin{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 688rpx;
}
.agin image{
  width: 16rpx;
  height: 16rpx;
  vertical-align: middle;
}
.agin .bdtext{
  font-size: 24rpx;
  margin-left: 80rpx;
  padding: 40rpx 0;
}
.agin .guang{
  display: flex;
  align-items: center;
}
.list checkbox{
  margin-right: 30rpx;
}
.list .wx-checkbox-input{
  border-radius: 50%;
  width: 30rpx;
  height: 30rpx;
  border: 4rpx solid #BEBEBE;
}
.list .wx-checkbox-input-checked{
  background-color: red;
  border: 4rpx solid red;
}
.list-main .item{
  background-color: white;
  border-radius: 40rpx;
  padding: 30rpx;
}
.dianpu {
  display: flex;
  align-items: center;
}
.dianpu .name image{
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  margin-bottom: 2rpx;
}
.produce,.pro-main{
  display: flex;
  flex-wrap: wrap;
}
.pro-main .proimage{
  width: 200rpx;
  height: 200rpx;
  margin-right: 20rpx;
}
.checkgood{
  display: flex;
  align-items: center;
  height: 200rpx;
}
.info .title{
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
  display: -webkit-box;
  overflow: hidden;
  margin-bottom: 10rpx;
  width: 390rpx;
}
.tabs image{
  width: 16rpx;
  height: 16rpx;
  vertical-align: middle;
}
.fuwu {
  display: flex;
  background-color: #F2F2F2;
  border-radius: 20rpx;
  font-size: 28rpx;
  padding: 0 8rpx;
  margin-bottom: 10rpx;
}
.fuwu .tuijian{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-right: 10rpx;
  width: 245rpx;
}
.cuxiao{
  margin-bottom: 10rpx;
  display: flex;
  font-size: 28rpx;
}
.cuxiao .tabbtn{
  width: 128rpx;
  background-color: #F2F2F2;
  border-radius: 40rpx;
  padding-left: 16rpx;
  margin-right: 20rpx;
}
.cuxiao .baitiao{
  color: red;
}
.yuyue{
  color: red;
  font-size: 28rpx;
}
.prinum{
  display: flex;
  justify-content: space-between;
  width: 390rpx;
  align-items: center;
  margin-bottom: 20rpx;
}
.price{
  color: red;
  display: flex;
  align-content: center;
  font-weight: bold;
}
.price .fuhao{
  font-size: 24rpx;
}
.price .jiage{
  font-size: 34rpx;
}
.price .xiaoshu{
  font-size: 24rpx;
}
.price .xinrenjia{
  width: 60rpx;
  height: 30rpx;
  margin-right: 4rpx;
}
.price .cgprice{
  width: 16rpx;
  height: 16rpx;
}
.prinum .num{
  display: flex;
  align-items: center;
  height: 40rpx;
}
.num input{
  width: 80rpx;
  padding: 10rpx 0;
  background-color: #F2F2F2;
  text-align: center;
  margin: 0 10rpx;
  min-height: 0;
  height: 20rpx !important;
}
.jianhao{
  width: 20rpx;
  height: 0px;
  border: 2rpx solid black;
}
.jiahao{
  font-weight: bold;
  margin-bottom: 6rpx;
}
.guanzhu{
  font-size: 24rpx;
  text-align: right;
}
.guanzhu .shuxian{ 
  margin: 0 10rpx;
  font-size: 22rpx;
  color: #BEBEBE;
}
.jiesuan{
  background-color: rgba(255, 255, 255, 0.9);
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 5;
  width: 750rpx;
}
.jiesuan .jscheck{
  margin-right: 20rpx;
}
.jiesuanbox{
  display: flex;
  justify-content: space-between;
  align-content: center;
  margin: 0 30rpx;
  padding: 10rpx 0;
  height: 68rpx;
  line-height: 68rpx;
}
.sumprice{
  display: flex;
  align-content: center;
  font-size: 36rpx;
  height: 68rpx;
  line-height: 68rpx;
}
.zongjinum{
  font-weight: bold;
}
.tojiesuan{
  padding: 10rpx 10rpx;
  background-color: red;
  color: white;
  border-radius: 40rpx;
  margin-left: 20rpx;
  height: 48rpx;
  line-height: 48rpx;
}
.jdtupian{
  width: 730rpx;
  margin: 0 10rpx;
  height: 169rpx;
  font-size: 0;
  margin-bottom: 40rpx;
}
.division{
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  margin-bottom: 60rpx;
}
.division .line{
  -webkit-box-flex: 1;
  -webkit-flex: 1 1 auto;
  -ms-flex: 1 1 auto;
  flex: 1 1 auto;
  height: 4rpx;
  background-color: #ccc;
}
.mayb{
  padding-bottom: 6rpx;
  -webkit-box-flex: 0;
  -webkit-flex: 0 1 auto;
  -ms-flex: 0 1 auto;
  flex: 0 1 auto;
  color: #999;
  position: relative;
  padding: 0 50rpx;
}
.mayb::after{
  content: '';
  position: absolute;
  right: 0;
  top: 50%;
  border-radius: 100%;
  margin-top: -6rpx;
  width: 10rpx;
  height: 10rpx;
  background: #ccc;
}
.mayb::before{
  content: '';
  position: absolute;
  left: 0;
  top: 50%;
  border-radius: 100%;
  margin-top: -6rpx;
  width: 10rpx;
  height: 10rpx;
  background: #ccc;
}
.goods{
  padding: 10rpx;
}
.goods::after{
  content: "";
  display: block;
  clear: both;
}
.goodnav{
  width: 50%;
  float: left;
  margin-bottom: 40rpx;
}
.gwrap{
  margin: 0 10rpx;
  background-color: white;
  border-radius: 20rpx;
}
.goodimg image{
  width: 100%;
  height: 348rpx;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}
.goodinfo{
  padding: 10rpx;
}
.goodtitle{
  overflow: hidden;
  color: #434343;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  word-break: break-all;
  -webkit-box-orient: vertical;
}
.tabcont{
  background-color: #F2F2F2;
  display: flex;
  align-items: center;
  width: 200rpx;
  padding-left: 4px;
  margin: 10rpx 0;
}
.tabcont image{
  width: 34rpx;
  height: 34rpx;
  margin-right: 4rpx;
}
.miaosha{
  width: 70rpx;
  height: 40rpx;
  vertical-align: middle;
}
.miaoshabox{
  height: 40rpx;
  padding-top: 4rpx;
  margin-left: 10rpx;
}
.pricebox .goodfuhao{
  font-size: 36rpx;
}
.pricebox .goodjiage{
  font-size: 46rpx;
}
.pricebox .goodxiaoshu{
  font-size: 36rpx;
}
.pinglun{
  max-width: 100%;
  color: #999;
  overflow: hidden;
  white-space: nowrap;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
  padding-bottom: 20rpx;
}